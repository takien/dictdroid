<?xml version="1.0" encoding="UTF-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="false">
        <shape>
            <gradient android:startColor="#fff8e98a" android:endColor="#fff8e98a" android:angle="90.0" android:centerY="0.5" android:centerColor="#fffbf5ca" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffe5cf33" android:endColor="#ffe5cf33" android:angle="90.0" android:centerY="0.5" android:centerColor="#fff1e7a2" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_pressed="true">
        <shape>
            <gradient android:startColor="#fff8e98a" android:endColor="#fff8e98a" android:angle="90.0" android:centerY="0.5" android:centerColor="#fffbf5ca" />
        </shape>
    </item>
</selector>